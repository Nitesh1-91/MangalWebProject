<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="Creative - Bootstrap 3 Responsive Admin Template">
    <meta name="author" content="GeeksLabs">
    <meta name="keyword" content="Creative, Dashboard, Admin, Template, Theme, Bootstrap, Responsive, Retina, Minimal">
    <link rel="shortcut icon" href="~/img/favicon.png">

    <link rel='stylesheet' type='text/css' href='http://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700,800'>
    <link rel="stylesheet" type="text/css" href="http://fonts.googleapis.com/css?family=Roboto:400,500,700,300">
    <!-- Required Plugin CSS -->
    @*<link rel="stylesheet" type="text/css" href="~/Content/assets/js/utility/highlight/styles/googlecode.css">*@
    <!-- Required Plugin CSS -->
    <link rel="stylesheet" type="text/css" href="~/Content/vendor/plugins/datepicker/css/bootstrap-datetimepicker.min.css">
    <link rel="stylesheet" type="text/css" href="~/Content/vendor/plugins/daterange/daterangepicker.css">
    <link rel="stylesheet" type="text/css" href="~/Content/vendor/plugins/colorpicker/css/bootstrap-colorpicker.min.css">
    <link rel="stylesheet" type="text/css" href="~/Content/vendor/plugins/tagmanager/tagmanager.css">
    <!-- Theme CSS -->
    @*<link rel="stylesheet" type="text/css" href="~/Content/assets/skin/default_skin/css/theme.css">
    <link rel="stylesheet" type="text/css" href="~/Content/assets/skin/default_skin/css/responsive.css">*@
    <!-- Favicon -->
    <link rel="shortcut icon" href="~/Content/assets/img/favicon.ico">
    <link rel="stylesheet" type="text/css" href="~/Content/assets/admin-tools/admin-forms/css/admin-forms.css">
    <link rel="stylesheet" type="text/css" href="~/Content/assets/my-style.css">
    <script type="text/javascript" src="~/Content/assets/js/includer.js"></script>
    <title>Mangal Fincorp</title>
    <!-- Bootstrap CSS -->
    <link href="~/Content/css/bootstrap.min.css" rel="stylesheet">
    <!-- bootstrap theme -->
    <link href="~/Content/css/bootstrap-theme.css" rel="stylesheet">
    <!--external css-->
    <!-- font icon -->
    <link href="~/Content/css/elegant-icons-style.css" rel="stylesheet" />
    <link href="~/Content/css/font-awesome.min.css" rel="stylesheet" />
    <!-- Custom styles -->
    <link href="~/Content/css/style.css" rel="stylesheet">
    <link href="~/Content/css/style-responsive.css" rel="stylesheet" />
    @*@Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")*@
</head>
<body>
    <!-- container section start -->
    <section id="container" class="">
        <!--header start-->
        <header class="header dark-bg">
            <div class="toggle-nav">
                <div class="icon-reorder tooltips" data-original-title="Toggle Navigation" data-placement="bottom"><i class="icon_menu"></i></div>
            </div>
            <!--logo start-->
            <a href="index.html" class="logo">Mangal Fincorp <span class="lite"></span></a>
            <!--logo end-->
            @*<div class="nav search-row" id="top_menu">
                    <!--  search form start -->
                    <ul class="nav top-menu">
                        <li>
                            <form class="navbar-form">
                                <input class="form-control" placeholder="Search" type="text">
                            </form>
                        </li>
                    </ul>
                    <!--  search form end -->
                </div>*@
            @Html.Action("Header","Base")
        </header>
        <!--header end-->
        <!--sidebar start-->
        <aside>
            @Html.Action("Menu","Base")
        </aside>
        <!--sidebar end-->
        <!--main content start-->
        <section id="main-content">
            <section class="wrapper">
                @*<div class="row">
                        <div class="col-lg-12">
                            <h3 class="page-header"><i class="fa fa fa-bars"></i> Pages</h3>
                            <ol class="breadcrumb">
                                <li><i class="fa fa-home"></i><a href="index.html">Home</a></li>
                                <li><i class="fa fa-bars"></i>Pages</li>
                                <li><i class="fa fa-square-o"></i>Pages</li>
                            </ol>
                        </div>
                    </div>*@
                <!-- page start-->
                @RenderBody()
                <!-- page end-->
            </section>
        </section>
        <!--main content end-->
        @Html.Action("Footer","Base")
    </section>

    <div class="modal fade" id="saveModal" role="dialog">
        <div class="modal-dialog  modal-sm">
            <div class="modal-content">
                <div class="modal-header">
                    <button aria-hidden="true" data-dismiss="modal" class="close" type="button">×</button>
                    <h4 class="modal-title">Save Record</h4>
                </div>
                <div class="modal-body">
                    <p>Record saved successfully. </p>
                </div>
                <div class="modal-footer">

                    <button class="btn btn-primary" data-dismiss="modal" type="button">Close</button>
                </div>
            </div><!-- /.modal-content -->
        </div><!-- /.modal-dialog -->
    </div>
    <div class="modal fade" id="updateModal" role="dialog">
        <div class="modal-dialog  modal-sm">
            <div class="modal-content">
                <div class="modal-header">
                    <button aria-hidden="true" data-dismiss="modal" class="close" type="button">×</button>
                    <h4 class="modal-title">Update Record</h4>
                </div>
                <div class="modal-body">
                    <p>Record updated successfully. </p>
                </div>
                <div class="modal-footer">

                    <button class="btn btn-primary" data-dismiss="modal" type="button">Close</button>
                </div>
            </div><!-- /.modal-content -->
        </div><!-- /.modal-dialog -->
    </div>
    <div class="modal fade" id="DeleteSuccessModal" role="dialog">
        <div class="modal-dialog  modal-sm">
            <div class="modal-content">
                <div class="modal-header">
                    <button aria-hidden="true" data-dismiss="modal" class="close" type="button">×</button>
                    <h4 class="modal-title">Delete Record</h4>
                </div>
                <div class="modal-body">
                    <p>Record deleted successfully. </p>
                </div>
                <div class="modal-footer">

                    <button class="btn btn-primary" data-dismiss="modal" type="button">Close</button>
                </div>
            </div><!-- /.modal-content -->
        </div><!-- /.modal-dialog -->
    </div>
    <div class="modal fade" id="addModal" role="dialog">
        <div class="modal-dialog  modal-sm">
            <div class="modal-content">
                <div class="modal-header">
                    <button aria-hidden="true" data-dismiss="modal" class="close" type="button">×</button>
                    <h4 class="modal-title">Add New Record</h4>
                </div>
                <div class="modal-body">
                    <p>Record saved successfully. </p>
                </div>
                <div class="modal-footer">

                    <button class="btn btn-primary" data-dismiss="modal" type="button">Close</button>
                </div>
            </div><!-- /.modal-content -->
        </div><!-- /.modal-dialog -->
    </div>
    <div class="modal fade" id="deleteModal" role="dialog">
        <div class="modal-dialog  modal-sm">
            <div class="modal-content">
                <div class="modal-header">
                    <button aria-hidden="true" data-dismiss="modal" class="close" type="button">×</button>
                    <h4 class="modal-title">Delete Record</h4>
                </div>
                <div class="modal-body">
                    <p>Are you Sure Delete your Record? </p>
                </div>
                <div class="modal-footer">

                    <button class="btn btn-primary" id="btnDeleteConfirm" data-dismiss="modal" type="button">Yes</button>
                    <button class="btn btn-primary" data-dismiss="modal" type="button">No</button>
                </div>
            </div><!-- /.modal-content -->
        </div><!-- /.modal-dialog -->
    </div>
    <div class="modal fade" id="closeModal" role="dialog">
        <div class="modal-dialog  modal-sm">
            <div class="modal-content">
                <div class="modal-header">
                    <button aria-hidden="true" data-dismiss="modal" class="close" type="button">×</button>
                    <h4 class="modal-title">Cancel Record</h4>
                </div>
                <div class="modal-body">
                    <p>Are you Sure Cancel your Record? </p>
                </div>
                <div class="modal-footer">
                    <button class="btn btn-primary" data-dismiss="modal" type="button">Yes</button>
                    <button class="btn btn-primary" data-dismiss="modal" type="button">No</button>
                </div>
            </div><!-- /.modal-content -->
        </div><!-- /.modal-dialog -->
    </div>
    <div class="modal fade bs-example-modal-lg" id="editModal" role="dialog">
        <button type="button" class="close1 white" data-dismiss="modal" aria-hidden="true">×</button>
        <div class="modal-dialog modal-lg" id="diveditfilldata">
        </div>
    </div>
    <div class="modal fade bs-example-modal-lg" id="viewModal" role="dialog">
        <button type="button" class="close1 white" data-dismiss="modal" aria-hidden="true">×</button>
        <div class="modal-dialog modal-lg" id="divviewfilldata">
        </div>
    </div>
    <div class="modal fade bs-example-modal-lg" id="viewformModal" role="dialog">
        <div class="modal-dialog modal-lg" id="divviewForm">
        </div>
    </div>
    <!-- container section end -->
    <!-- javascripts -->
    <script src="~/Content/js/jquery.js"></script>
    <script src="~/Content/js/bootstrap.min.js"></script>
    <!-- nice scroll -->
    <script src="~/Content/js/jquery.scrollTo.min.js"></script>
    <script src="~/Content/js/jquery.nicescroll.js" type="text/javascript"></script>
    <!--custome script for all page-->
    <script src="~/Content/js/scripts.js"></script>

    @*@Scripts.Render("~/Scripts/js")*@
    @*@Scripts.Render("~/bundles/bootstrap")*@
    @RenderSection("scripts", required: false)
    @RenderSection("javascriptsection", false)
</body>
</html>
